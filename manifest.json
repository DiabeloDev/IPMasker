{
  "manifest_version": 3,
  "name": "IP Masker",
  "version": "1.2",
  "description": "Zamazuje wszystkie adresy IP na stronach.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}